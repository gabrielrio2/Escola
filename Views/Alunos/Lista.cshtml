@model EscolaProjeto.Models.Aluno

@{
    ViewData["Title"] = "Listagem de Grupo";
    List<Aluno> listaGrupo = ViewBag.ListAluno;
}

<h1>Details</h1>

<div>
    <h4>Lista</h4>
    <hr />
     <table class="table">
        <tr>
            <td>Nome</td>
            
            <td>Matricula</td>
            <td>Cpf</td>
        </tr>
       
        @foreach (var g in listaGrupo)
        {
            <tr>
                <td>@g.Name</td>
                <td>@g.Matricula</td>
                <td>@g.Cpf</td>
            </tr>
            
        }

     </table>
</div>
<div>
    
    <a asp-action="Index">Back to List</a>
</div>
